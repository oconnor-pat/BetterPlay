{
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true
  },
  "postman.settings.dotenv-detection-notification-visibility": false,
  "codium.codeCompletion.enable": true,
  "cSpell.ignorePaths": [
    "**/*.pbxproj",
    "node_modules",
    "ios/Pods/**",
    "ios/build/**",
    "android/build/**",
    "android/app/build/**",
    "vendor/**",
    "**/*.p8",
    "**/*.plist",
    "**/*.xcworkspacedata",
    "**/*.xcscheme"
  ],
  "cSpell.words": [
    "comgooglemaps",
    "daddr",
    "datetimepicker",
    "directionsmode",
    "getifaddr",
    "notif",
    "Notifs",
    "pbxproj",
    "selectbox",
    "xcscheme",
    "xcworkspacedata"
  ]
}
